{
  "name": "wasi-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "start": "node build/server/bin/www.js",
    "dev:server": "tsc -w -p ./src/server & nodemon --delay 5 build/server/bin/www.js",
    "dev:client": "ng serve --disable-host-check --open --serve-path /challenge/ --proxy-config proxy.conf.json",
    "build": "npm-run-all -p build:*",
    "build:server": "tsc -p ./src/server",
    "build:client": "ng build --prod --deploy-url /challenge/dist/",
    "i18n": "ng xi18n --output-path src/client/locale",
    "e2e": "ng e2e",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "ng lint",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "format:fix": "prettier --write",
    "format:write": "prettier --write \"**/*{.ts,.js,.json,.css,.scss}\"",
    "format:check": "prettier --list-different \"**/*{.ts,.js,.json,.css,.scss}\"",
    "commitmsg": "commitlint -e",
    "precommit": "lint-staged",
    "bundle-report": "webpack-bundle-analyzer build/dist/stats.json",
    "compodoc": "./node_modules/.bin/compodoc -p src/client/tsconfig.app.json",
    "jest": "jest --updateSnapshot"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "read-excel-file": "^2.0.6",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.2",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
